div#header
    a(href='/public/artwork')
        img#logo(src="/logo.png", alt="logo")
    | 
    if accountType === 'patron'
        a(href="/patron/home") Profile
    else if accountType === 'artist'
        a(href="/artist/home") Profile
    | 
    a#notification-link(href="/notification", data-artist-id=artistId, data-user-id=userId) Notification
    |
    a(href="/logout") Logout
    | 
    link(rel='icon', href='/favicon.ico')